{
    "version": 2,
    "builds": [
        {
            "src": "./bin/www",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "./bin/www"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}